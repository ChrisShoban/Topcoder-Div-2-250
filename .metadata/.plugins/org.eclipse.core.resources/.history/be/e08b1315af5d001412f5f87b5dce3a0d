public class Highscore {

	public int getRank(int[] scores, int newscore, int places) {
		if(scores.length == 0 && places > 1) {
			return 1;
		}
		int rank = 1;
		for(int i = 0; i < places; i++) {
			if(i > 0 && scores[i] != scores[i - 1]) {
				rank++;
			}
			if(newscore == scores[i]) {
				if(i == places - 1) {
					return -1;
				}
				else {
					return rank;
				}
			}
			else if(newscore > scores[i]) {
				return rank;
			}
		}
		return 0;
	}
}
